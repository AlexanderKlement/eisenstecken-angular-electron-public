{
  "name": "eisenstecken-eibel",
  "version": "1.1.16",
  "description": "Eisenstecken internal administration software",
  "homepage": "https://kivi.bz.it",
  "author": {
    "name": "Kivi",
    "email": "info@kivi.bz.it"
  },
  "keywords": [
    "angular",
    "angular 13",
    "electron",
    "electron 16",
    "nodejs",
    "typescript",
    "playwright",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "app/main.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o --poll=2000",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "electron:deploy": "npm run build:prod && electron-builder build --publish=always",
    "electron:dev": "npm run build:dev && electron-builder build --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "package:msi": "node build_installer_msi.js",
    "generate": "openapi-generator-cli generate -g typescript-angular -i https://api.eisenstecken.kivi.bz.it/openapi.yaml -o src/api/openapi --additional-properties=supportsES6=true,withSeparateModelsAndApi=true,ngVersion=14.3.0"
  },
