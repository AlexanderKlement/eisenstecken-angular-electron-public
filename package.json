{
  "name": "eisenstecken-eibel",
  "version": "2.0.6",
  "description": "Eisenstecken internal administration software",
  "homepage": "https://kivi.bz.it",
  "author": {
    "name": "Kivi",
    "email": "info@kivi.bz.it"
  },
  "keywords": [
  ],
  "main": "app/main.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o --poll=2000",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "electron:deploy": "npm run build:prod && electron-builder build --publish=always",
    "electron:dev": "npm run build:dev && electron-builder build --publish=never",
    "electron:beta": "npm run build:prod && electron-builder -c electron-builder.beta.json --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "package:msi": "node build_installer_msi.js",
    "generate": "openapi-generator-cli generate -g typescript-angular -i https://api.app.eisenstecken.it/beta/openapi.yaml -o src/api/openapi --additional-properties=supportsES6=true,withSeparateModelsAndApi=true,ngVersion=20.2.0"
  },
  "dependencies": {
    "@angular-eslint/eslint-plugin": "20.6.0",
    "@angular-eslint/schematics": "20.5.1",
    "@angular/animations": "20.2.0",
    "@angular/cdk": "20.2.0",
    "@angular/common": "20.2.0",
    "@angular/compiler": "20.2.0",
    "@angular/core": "20.2.0",
    "@angular/forms": "20.2.0",
    "@angular/language-server": "20.2.2",
    "@angular/language-service": "20.2.0",
    "@angular/material": "20.2.0",
    "@angular/platform-browser": "20.2.0",
    "@angular/platform-browser-dynamic": "20.2.0",
    "@angular/router": "20.2.0",
    "@angular/service-worker": "20.2.0",
    "@sentry/angular": "10.5.0",
    "@sentry/tracing": "7.120.3",
    "angular-calendar": "0.29.0",
    "angular-disable-browser-back-button": "2.0.0",
    "canvas-confetti": "1.4.0",
    "date-fns": "2.28.0",
    "dayjs": "1.11.19",
    "electron-squirrel-startup": "1.0.0",
    "material-design-icons": "3.0.1",
    "ng-flex-layout": "20.0.0",
    "ngx-material-timepicker": "13.1.1",
    "rxjs": "^7.4.0",
    "tslib": "^2.1.0",
    "yaml": "1.10.2",
    "zone.js": "0.15.1"
  },
  "repository": {
